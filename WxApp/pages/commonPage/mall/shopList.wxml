<include src="/pages/commonPage/common.wxml"/><block wx:if="{{config.shop.shopListIsSearch == 1}}"><form report-submit="true" bindsubmit="shopListEvent" class="shop_top_search" data-types="shopListSearch"><view class="bgfff p20 box_b overHidden flex_between"><view class="mewsSearch"><view class="pos news_search_box"><image wx:if="{{iconStatus}}" src="https://www.haojian.cn/wximg/search.png" class="seaIcon"></image><input bindconfirm="shopListEvent" data-types="shopListSearch" name="keyword" type="text" confirm-type="search" placeholder="搜索"/></view></view><button class="searchButton" style="background:#{{peise.mcolor}}" form-type="submit">搜索</button></view></form></block><block wx:if="{{shopTempName}}"><block wx:if="{{isShowShopClass == 0 || shopSearch}}"><block wx:if="{{shopTempName === 'side'}}"><include src="filterBar.wxml"/><block wx:if="{{!shopSearch && config.shop.shopListIsBanner == 1}}"><banner-html data="{{banner}}"></banner-html></block><include src="shopCategories2.wxml"/></block><block wx:else><include src="filterBar.wxml"/><block wx:if="{{!shopSearch && config.shop.shopListIsBanner == 1}}"><banner-html data="{{banner}}"></banner-html></block><include src="tempAll.wxml"/></block></block><block wx:else><block wx:if="{{shopTempName === 'side'}}"><include src="filterBar.wxml"/><include src="shopCategories.wxml"/></block><view wx:else scroll-top="{{scrollTop}}"><view class="top_line"></view><view class="mallNavFxied f28" wx:if="{{navList.length > 0}}" style="top: {{config.shop.shopListIsSearch == 1 ? '100rpx' : '0rpx'}}"><scroll-view class="scroll-view_H" scroll-x="true" style="width: 100%" scroll-into-view="{{mall.toView}}"><view data-current="{{index}}" wx:for="{{navList}}" wx:key="" id="category_{{index}}" catchtap="shopListEvent" data-types="swichNav" class="top-bar-item txt_center {{index == currentTab ? 'top-bar-active' : ''}}" data-categoryid="{{item.id}}" style="color:#{{currentTab === index ? peise.mcolor : ''}}">{{item.name}}<view class="top-bar-item_line" style="background:#{{currentTab === index ? peise.acolor : ''}}"></view></view></scroll-view></view><block wx:if="{{seconNavL && seconNavL.length > 0}}"><view class="madel_box" catchtap="shopListEvent" data-types="closeNav"></view><view class="secon_nav_box_1"><view wx:for="{{seconNavL}}" wx:key="" class='secon_nav_item_1 {{mall.seconNav == item.id ? "active" : ""}}' catchtap="shopListEvent" data-types="seconNav" data-categoryid="{{item.id}}" data-current="{{index}}">{{item.name}}</view></view></block><include src="filterBar.wxml"/><block wx:if="{{config.shop.shopListIsBanner == 1}}"><banner-html data="{{banner}}"></banner-html></block><include src="tempAll.wxml"/></view></block></block><image wx:if="{{showGoTop && isPageScrollTo}}" bindtap="onBackTop" style="bottom:120rpx" class="scrolltop" src="https://www.haojian.cn/wximg/top_back.png"></image><navigator wx:if="{{cartNum > 0}}" class="cartNumIcon" hover-class="none" url="/pages/mall/pages/shopCart/shopCart"><view class="cartNum" style="{{cartNum >= 100 ? 'padding:0 8rpx' : 'width:30rpx'}}">{{cartNum}}</view><image class="cartNum_img" src="https://www.haojian.cn/wximg/mall/cart.png"></image></navigator><include src="/pages/commonPage/mall/shopSkus.wxml"/>