<include src="/pages/commonPage/common.wxml"/><block wx:if='{{news_list_view_type === "circle"}}'><block wx:if="{{requestStatus}}"><view wx:if="{{list}}"><view class="nm_1 bgfff mt20" wx:for="{{list}}" wx:key=""><include src="circleTemp.wxml"/></view></view><block wx:if="{{list.length == 0}}"><view class="null">没有数据</view><copyright-html data="{{config}}"></copyright-html></block></block></block><block wx:else><block wx:if="{{banner.length > 0}}"><form report-submit="true" bindsubmit="newsEvent" data-types="submitNewsSearchBtn"><view wx:if="{{newsIssearch == 1}}" class="bgfff p20 box_b overHidden flex_between"><view class="mewsSearch"><view class="pos news_search_box"><image wx:if="{{iconStatus}}" src="https://www.haojian.cn/wximg/search.png" class="seaIcon"></image><input name="keyword" type="text" value="{{searchVal}}" bindfocus="hideIcon" bindblur="newsBlurInput" bindconfirm="newsEvent" data-types="newsSearchBtn" confirm-type="search" placeholder="搜索"/></view></view><button class="searchButton" style="background:#{{peise.mcolor}}" form-type="submit">搜索</button></view></form><banner-html data="{{banner}}"></banner-html><view wx:if="{{navList.length > 0}}" id="tab"><view class="top_bar f28 {{isTabFixed ? 'fixed_top' : ''}}"><include src="nav.wxml"/></view></view></block><block wx:if="{{banner.length == 0}}"><block wx:if="{{navList.length > 0}}"><view class="top_bar f28 newsNavFxied"><include src="nav.wxml"/></view><view class="space120"></view></block><view wx:if="{{newsIssearch == 1}}" class="mewsSearch"><view class="pos news_search_box bgfff"><image wx:if="{{iconStatus}}" src="https://www.haojian.cn/wximg/search.png" class="seaIcon"></image><input type="text" value="{{searchVal}}" bindfocus="hideIcon" bindblur="newsBlurInput" bindconfirm="newsEvent" data-types="newsSearchBtn" confirm-type="search" placeholder="搜索"/></view></view></block><block wx:if="{{requestStatus}}"><block wx:if="{{news_list_view_type === 'normal'}}"><view class="mt20" wx:if="{{list.length > 0 }}"><block wx:for="{{list}}" wx:key=""><include src="temp.wxml"/></block></view></block><block wx:elif="{{news_list_view_type === 'pic_left'}}"><view class="mt20" wx:if="{{list.length > 0}}"><block wx:for="{{list}}" wx:key=""><include src="pic_left.wxml"/></block></view></block><block wx:elif="{{news_list_view_type === '2col'}}"><view wx:if="{{list.length > 0}}" class="flex_warp mt20 pt30 bgfff"><block wx:for="{{list}}" wx:key=""><include src="2col.wxml"/></block></view></block><block wx:elif="{{news_list_view_type === '1col_large'}}"><view class="mt20" wx:if="{{list.length > 0}}"><block wx:for="{{list}}" wx:key=""><include src="1col_large.wxml"/></block></view></block><view wx:if="{{list.length == 0}}" class="null" style='{{banner.length > 0 ? "top:70%" : ""}}'>没有数据</view><view wx:if="{{list.length != 0 && !hasNext}}" class="noNore">没有更多数据了</view><block wx:if="{{list.length == 0}}"><copyright-html data="{{config}}"></copyright-html></block></block></block><image wx:if="{{showGoTop && isPageScrollTo}}" bindtap="onBackTop" class="backTop" src="https://www.haojian.cn/wximg/top_back.png"></image>