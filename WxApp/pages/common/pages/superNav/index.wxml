<view><view class="searchWaver"><view class="searchBg"><image src="https://www.haojian.cn/wximg/search.png"></image><input class="search" placeholder="请输入关键字"/><text class="searchBtn">搜索</text></view></view><swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" indicator-active-color="#fff" class="banner"><block wx:for="{{imgUrls}}" wx:key=""><swiper-item><navigator url="{{item.link}}" hover-class="navigator-hover"><image src="{{item.url}}" class="slide-image" width="355" height="300"/></navigator></swiper-item></block></swiper><view class="introduce"><view class="f30 c3 txt_center mt20">- {{introduce}} -</view><scroll-view scroll-x="true" class="column"><view class="columnSon" wx:for="{{columns}}" wx:for-item="item" wx:key="" style="background-image: url('{{theme.theme_img}}')"><view class="flex_between flex_items" catchtap="viewCode" data-index="{{index}}" data-types="recommend"><view class="flex1 overHidden"><view class="f30 cf">{{item.nav_name}}</view><view class="f24 cf text_overflow">{{item.description}}</view></view><image class="preview flex_shrink" src="{{item.icon}}"></image></view></view></scroll-view></view><scroll-view wx:if="{{navs.length > 0}}" scroll-x="true" class="nav"><view data-idx="0" class='item plr20 {{currentTab == 0 ? "cff85" : ""}}' bindtap="navbarTap"><view class="flex_center flex_items pos"><view>全部</view><view wx:if="{{currentTab == 0}}" class="active"></view></view></view><view wx:for="{{navs}}" wx:for-item="nav" data-idx="{{nav.id}}" class="item plr20 {{currentTab==nav.id ? 'cff85' : ''}}" wx:key="" bindtap="navbarTap"><view class="flex_center flex_items pos"><view>{{nav.nav_menu}}</view><view wx:if="{{currentTab==nav.id}}" class="active"></view></view></view></scroll-view><view class="list"><block wx:if="{{lists.length > 0}}"><view wx:for="{{lists}}" wx:for-item="list" wx:for-index="" wx:key="" class="listMenu flex_start flex_items" bindtap="viewCode" data-index="{{index}}" data-types="list" name="1"><image class="listImg" src="{{list.icon}}"></image><view class="main"><view class="f32 c3">{{list.nav_name}}</view><view class="f26 c6 mt10 clamp2">{{list.description}}</view></view></view></block><view wx:else class="noComment">暂无记录</view></view></view><view class='tankuang {{showtk ? "show" : "hide"}}' catchtap="closeTk"></view><view class='model_box {{showtk ? "show" : "hide"}}'><view class="model_tit">{{selCode.nav_name}}</view><image catchtap="viewBigImg" src="{{selCode.qr_code}}"></image><view class="f26 c9">点击图片并长按识别图中小程序码</view></view>