<block wx:if="{{requestStatus}}"><include src="/pages/commonPage/common.wxml"/><view class="flex_column"><view class="flex_start bgfff p20 box_b"><view bindtap="settleEvent" data-types="city" class="box_b f26 flex_start flex_shrink pr30 flex_items"><view class="pr10">{{cityName}}</view><image src="https://www.haojian.cn/wximg/down_icon.png" class="settle_down_icon"></image></view><view data-types="search" class="mewsSearch" catchtap="settleEvent"><image src="https://www.haojian.cn/wximg/search.png" class="seaIcon"></image><view class="f26 cc">搜索</view></view></view><banner-html data="{{banner}}"></banner-html><block wx:if="{{typeName.length > 1}}"><swiper class="topType bgfff" indicator-dots="true" interval="3000" duration="1000" indicator-color="#cccccc" indicator-active-color="#666666"><block wx:for="{{typeName}}" wx:key="" wx:for-item="typeList"><swiper-item class="flex_warp bgfff"><view wx:for="{{typeList}}" wx:key="" class="smct_cls_item txt_center"><view catchtap="settleEvent" data-item="{{item}}" data-types="class"><image lazy-load="true" class="settle_smct_cls_img" mode="scaleToFill" src="{{item.logo.thumb_small_url}}"></image><view class="f24 c3 smct_cls_txt txt_center">{{item.name}}</view></view></view></swiper-item></block></swiper></block><block wx:else><view class="flex_warp bgfff ptb20" wx:for="{{typeName}}" wx:key="" wx:for-item="typeList"><view wx:for="{{typeList}}" wx:key="" class="smct_cls_item txt_center"><view catchtap="settleEvent" data-item="{{item}}" data-types="class"><image lazy-load="true" class="settle_smct_cls_img" mode="scaleToFill" src="{{item.logo.thumb_small_url}}"></image><view class="f24 c3 smct_cls_txt txt_center">{{item.name}}</view></view></view></view></block><view wx:if="{{featured.length > 0}}" class="mtb20 bgfff tuijian_box"><view class="tit f30 c3 txt_center mt30">- 精选{{busConfig.isCompany}} -</view><view class="scroll_box"><scroll-view scroll-x style="width: auto;overflow:hidden"><navigator class="tuijian_item" wx:for="{{featured}}" wx:key="" url="/pages/settleIn/pages/storeDetail/storeDetail?id={{item.id}}" hover-class="none"><view><image lazy-load="true" class="tuijian_img" src="{{item.logo.thumb_medium_url}}" mode="scaleToFill"></image><view class="f24 c6 mt20 mb40 txt_center text_overflow">{{item.businessName}}</view></view></navigator></scroll-view></view></view><include src="/pages/commonPage/settleIn/list.wxml"/></view><view class="xuanfuBtn"><image wx:if="{{showGoTop && isPageScrollTo}}" bindtap="settleEvent" data-types="onBackTop" class="fixedBtn_1" src="https://www.haojian.cn/wximg/top_back.png"></image><view class="settle_btn" catchtap="settleEvent" data-types="publishBtn" style="background: #{{peise.mcolor}}">入驻</view></view><view class="tankuang {{showSub[0]}}" catchtap="settleEvent" data-types="close" data-index="0"></view><view class="alter2 {{showSub[0]}}"><view class="alterTitle"><view class="alter2Close" bindtap="settleEvent" data-types="close" data-index="0"><image src="https://www.haojian.cn/wximg/close.png"></image></view><view class="txt_center f34 c3 bbe9s">选择二级分类</view></view><view class="fenlei_box flex_start flex_warp"><view class="f26 c6 text_overflow fenlei_item" wx:for="{{subList}}" wx:key="" data-id="{{item.id}}" catchtap="settleEvent" data-types="subClass">{{item.name}}</view></view></view><view class="tankuang {{showSub[1]}}" catchtap="settleEvent" data-types="close" data-index="1"></view><view class="alter2 {{showSub[1]}}"><view class="alterTitle"><view class="alter2Close" bindtap="settleEvent" data-types="close" data-index="1"><image src="https://www.haojian.cn/wximg/close.png"></image></view><view class="txt_center f34 c3 bbe9s">发布类型</view></view><view class="itemType" catchtap="settleEvent" data-types="publish" data-key="merchant">入驻店铺</view><view class="itemType bte9s" catchtap="settleEvent" data-types="publish" data-key="goods">发布商品</view></view></block>