<import src="../../../../wxParse/wxParse.wxml"/><block wx:if="{{requestStatus}}"><view class="detail_info_box bgfff"><image class="detail_img bte9s" mode="scaleToFill" src="{{detail.cover.original_url}}"></image><view class="plr30 mt40 mb30 f32 c3">{{detail.title}}</view><view class="detail_ys_time plr30 pb30 bbe9s flex_between flex_items"><view class="f22 c9">有效期：{{detail.start_time}}-{{detail.end_time}}</view><view wx:if="{{detail.fees && detail.fees.length > 1 && iphoneShow}}" class="f32 cff5">¥{{detail.minMoney}}起</view><view wx:elif="{{detail.fees && detail.fees.length === 1 && iphoneShow}}" class="f32 cff5">¥{{detail.fees[0].fee}}</view></view></view><view class="detail_fwb_box bgfff"><view class="mt10"><view wx:if="{{detail.info === null || detail.info === ''}}" class="noDetail">无简介信息</view><template is="wxParse" data="{{wxParseData:article.nodes}}"/></view></view><form report-submit bindsubmit="formSubmit"><view class="plr20"><view wx:if="{{detail.fees.length > 0}}" class="form_info_box"><view class="c0 f30 bold">收费类型</view><radio-group class="f28 c6" name="feeId" bindchange="checkFeeType"><view wx:for="{{detail.fees}}" wx:key="" class="flex_between flex_items mt40"><view>{{item.name}}</view><view class="flex_start flex_items flex_shrink"><view wx:if="{{iphoneShow}}" class="cff5 mr20">￥{{item.fee}}</view><radio value="{{index}}" color="#ff8542"/></view></view></radio-group></view><view class="form_info_box"><view class="c0 f30 bold mb40">填写信息</view><include src="/pages/commonPage/common/input/formTemp.wxml"/><view class="p10"><button wx:if="{{numberValue}}" class="submit_form_btn" form-type="submit">提交</button><block wx:else><button wx:if="{{btnPhoneNumber}}" bindgetphonenumber="bindPhoneNumber" data-types="button" open-type="getPhoneNumber" class="submit_form_btn">确认</button><view wx:else bindtap="bindPhoneNumber" data-types="importPhone" class="submit_form_btn">确认</view></block></view></view></view></form><view class="mb20 mt20"><official-account></official-account></view><block wx:if="{{adStatus}}"><ad unit-id="adunit-{{config.advert.form.detail}}" binderror="adError"></ad></block><copyright-html data="{{config}}"></copyright-html></block><block wx:if="{{payAndBingPhoneStatus}}"><include src="/pages/commonPage/payTemp.wxml"/><include src="/pages/commonPage/common/setPassword.wxml"/><include src="/pages/commonPage/common/bindPhone.wxml"/></block>