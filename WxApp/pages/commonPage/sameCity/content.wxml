<view class="sameCity_row" wx:if="{{list.length > 0}}"><block wx:if="{{config.catalog.isTitle == 1}}"><view wx:for="{{list}}" wx:key="" wx:for-index="listIndex" class="bgfff"><view class="plr30 flex_items bbe9s"><navigator class="flex_between flex_items" hover-class="none" url="/pages/sameCity/pages/detail/detail?id={{item.id}}&index={{listIndex}}"><view class="list_left_box flex_start flex_items"><view wx:if="{{item.is_top}}" class="lf_ding">顶</view><view wx:if="{{item.hongbao_pool}}" class='lf_hb {{item.is_top ? "ml5" : ""}}'>福利</view><view wx:if="{{item.catalog.name}}" catchtap="sameCityEvent" data-types="liexing" data-id="{{item.catalog.id}}" data-typesid="{{item.catalog.id}}" class="f32 ca4a6aa mlr10 box_b flex_shrink">{{item.catalog.name}}</view><view class='text_overflow ptb30 f30 c3 {{item.is_top || item.hongbao_pool ? "ml10" : ""}}'>{{item.title}}</view></view><view class="list_right_time box_b flex_shrink">{{item.datetime}}</view></navigator></view></view></block><block wx:else><view wx:for="{{list}}" wx:key="" wx:for-index="listIndex" class="bgfff mb20 pb20"><view class="flex_start pt30 pos"><view class="smct_row_lf pl30 pos"><image data-id="{{item.id}}" data-index="{{listIndex}}" data-types="detail" catchtap='{{item.uid == item.owner.id || item.status == "normal" ? "sameCityEvent" : ""}}' class="smct_row_lf_avatar" mode="scaleToFill" src="{{item.owner.avatar_url.thumb_small_url}}"></image><image wx:if="{{item.member.isMember}}" class="vip" src="https://www.haojian.cn/wximg/market/vip_{{item.member.grade}}.png"></image></view><image wx:if='{{item.status == "normal" && item.is_top}}' class="smct_sticky_img" src="https://www.haojian.cn/wximg/smct_sticky.png"></image><image wx:if='{{item.status === "wait_confirm"}}' class="smct_sticky_img" src="https://www.haojian.cn/wximg/shenhe.png"></image><view class="smct_row_rt pr30"><view data-id="{{item.id}}" data-index="{{listIndex}}" data-types="detail" catchtap='{{item.uid == item.owner.id || item.status == "normal" ? "sameCityEvent" : ""}}'><view class="flex_start flex_items"><view class="f26 c6 text_overflow">{{item.owner.displayname}}</view><view wx:if="{{item.hongbao_pool}}" class="flex_start flex_items ml20"><image class="hongbao" src="https://www.haojian.cn/wximg/tc/tcList5.png"></image><view class="cfd5666 f20 ml10">福利</view></view></view><view class="f20 cc">{{item.datetime}}</view></view><view wx:if='{{!item.is_top && item.uid == item.owner.id && item.status == "normal"}}' class="sticky_btn smct_row_rt_btn f22 cf" catchtap="sameCityEvent" data-types="zhiding" data-id="{{item.id}}" data-catalogid="{{item.catalog.id}}" data-index="{{listIndex}}">点击置顶</view><view wx:if="{{item.status == 'normal' && item.uid != item.owner.id}}" class="contact_btn smct_row_rt_btn f22 c3" catchtap="sameCityEvent" data-types="call" data-phone="{{item.customer_service.phone}}">联系Ta</view></view></view><view class="plr30 box_b"><view data-id="{{item.id}}" data-index="{{listIndex}}" data-types="detail" bindtap='{{item.uid == item.owner.id || item.status == "normal" ? "sameCityEvent" : ""}}' class="ptb30 flex_start"><view class="{{item.isMore ? 'clamp4' : ''}}"><text wx:if="{{item.catalog.name}}" catchtap="sameCityEvent" data-types="liexing" data-id="{{item.catalog.id}}" data-typesid="{{item.catalog.id}}" class="f32 ca4a6aa mr10">#{{item.catalog.name}}#</text><text class="lh42 f32 c3">{{item.content}}</text></view></view><view wx:if="{{item.content.length > 100}}"><view wx:if="{{item.isMore}}" catchtap="sameCityEvent" data-types="zhankaiMore" data-index="{{listIndex}}" class="newMore f32 pb30">展开更多</view><view wx:if="{{!item.isMore}}" catchtap="sameCityEvent" data-types="yincangMore" data-index="{{listIndex}}" class="newMore f32 pb30">收起</view></view><view wx:if="{{item.attachments.length > 0}}" class="flex_warp"><block wx:for="{{item.attachments}}" wx:key="" wx:for-item="img"><image lazy-load="true" bindtap="sameCityEvent" data-types="bigImg" data-index="{{listIndex}}" data-url="{{img.original_url}}" class="smct_info_img" mode="scaleToFill" src="{{img.thumb_small_url}}"></image></block><view data-id="{{item.id}}" data-index="{{listIndex}}" data-types="detail" catchtap='{{item.uid == item.owner.id || item.status == "normal" ? "sameCityEvent" : ""}}' class="flex1"></view></view><view wx:if="{{item.location.desc}}" catchtap="sameCityEvent" data-types="address" data-lat="{{item.location.lat}}" data-lng="{{item.location.lng}}" class="flex_start flex_items ptb20"><image class="smct_address" src="https://www.haojian.cn/wximg/tc/address.png"></image><view class="f24 c406bb2 text_overflow ml10">{{item.location.desc}}</view></view><view class="flex_around"><view class="flex_start flex_items p20" data-id="{{item.id}}" data-index="{{listIndex}}" data-types="detail" catchtap='{{item.uid == item.owner.id || item.status == "normal" ? "sameCityEvent" : ""}}'><image class="view_img" src="https://www.haojian.cn/wximg/tc/tcList1.png"></image><view class="ml10 f22 c9">{{item.view_count}}</view></view><view class="flex_start flex_items p20" data-id="{{item.id}}" data-index="{{listIndex}}" data-types="detail" catchtap='{{item.uid == item.owner.id || item.status == "normal" ? "sameCityEvent" : ""}}'><image class="comment_img" src="https://www.haojian.cn/wximg/tc/tcList2.png"></image><view class="ml10 f22 c9">{{item.comment_count}}</view></view><view wx:if="{{item.is_like}}" bindtap="sameCityEvent" data-types="dianzan" data-status="0" data-pubid="{{item.id}}" data-index="{{listIndex}}" class="flex_start flex_items p20"><image class="like_img" src="https://www.haojian.cn/wximg/tc/tcList31.png"></image><view class="ml10 f22 c9">{{item.like_count}}</view></view><view wx:else bindtap="sameCityEvent" data-types="dianzan" data-status="1" data-pubid="{{item.id}}" data-index="{{listIndex}}" class="flex_start flex_items p20"><image class="like_img" src="https://www.haojian.cn/wximg/tc/tcList3.png"></image><view class="ml10 f22 c9">{{item.like_count}}</view></view><view class="flex_start flex_items f22 c9 pos p20" catchtap="sameCityEvent" data-types="share" data-index="{{listIndex}}"><image class="share_img" src="https://www.haojian.cn/wximg/tc/tcList4.png"></image>分享<view class="shareBtn" wx:if='{{item.status === "normal"}}'><button class="be9s" open-type="share" data-index="{{listIndex}}" hover-class="none"></button></view></view></view></view></view></block></view><view wx:if="{{list.length != 0 && !hasNext}}" class="noNore">没有更多数据了</view>