<block wx:for="{{lists}}" wx:key=""><block wx:if="{{item.type === 'label'}}"><view class="f30 c3 mb10">{{item.name}}</view></block><view wx:if="{{item.type == 'single_line' || item.typeid == 0}}" class="ptb20"><view class="f32 c0 bold">{{item.title}}<text wx:if="{{item.isMust == 1}}" class="color-red f26">*</text></view><input bindinput="inputEvent" data-types="singleline" data-index="{{index}}" class="f28 inp form_choose_box mt10" placeholder="请输入{{item.title}}" name="{{item.id}}" value="{{item.value}}" data-inp="{{index}}"/></view><view wx:if="{{item.type == 'multiple_line' || item.typeid == 1}}" class="ptb20"><view class="f32 c0 bold">{{item.title}}<text wx:if="{{item.isMust == 1}}" class="color-red f26">*</text></view><view class="form_choose_box p20 box_b mt20"><view class="mulText f28" bindtap="inputEvent" data-types="textMultipleFocus" data-index="{{index}}" data-val="{{item.value}}" wx:if="{{!item.textMul}}">{{item.value}}</view><textarea class="mulText c0 f28" placeholder-class="c0 f28" wx:else bindblur="inputEvent" data-types="inputText" data-index="{{index}}" placeholder="请输入内容" value="{{item.value}}" focus name="{{item.id}}" maxlength="-1"></textarea></view></view><view wx:elif="{{item.type == 'single_choice' || item.typeid == 2}}" class="ptb20"><view class="f32 c0 bold">{{item.title}}<text wx:if="{{item.isMust == 1}}" class="color-red f26">*</text></view><view class="form_choose_box p20 box_b mt20"><radio-group bindchange="inputEvent" data-types="radioChange" data-index="{{index}}"><label wx:for="{{item.choice}}" wx:key="" wx:for-item="radio" wx:for-index="idx"><view class="ptb10 c0 f28 flex_start flex_items"><radio style="width:40rpx;height:40rpx" value="{{radio.name}}_{{idx}}" checked="{{radio.checked}}"></radio><view class="ml20">{{radio.name}}</view></view></label></radio-group></view></view><view wx:elif="{{item.type == 'multiple_choice' || item.typeid == 3}}" class="ptb20"><view class="f32 c0 bold">{{item.title}}<text wx:if="{{item.isMust == 1}}" class="color-red f26">*</text></view><view class="form_choose_box p20 box_b mt20"><checkbox-group bindchange="inputEvent" data-types="checkboxChange" data-index="{{index}}" name="{{item.id}}"><label wx:for="{{item.choice}}" wx:key="" wx:for-item="itemName" wx:for-index="idx"><view class="ptb20 c0 f28 flex_start flex_items"><checkbox style="width:40rpx;height:40rpx" checked="{{itemName.checked}}" value="{{itemName.name}}_{{idx}}"/><view class="ml20">{{itemName.name}}</view></view></label></checkbox-group></view></view><view wx:elif="{{item.type == 'picture' || item.typeid == 4}}" class="ptb20"><view class="f32 c0 bold">{{item.title}}<text wx:if="{{item.isMust == 1}}" class="color-red f26">*</text></view><view class="flex_warp p30 box_b mt20 form_choose_box"><view class="photo_upview" wx:for="{{item.value}}" wx:key="" wx:for-item="imgItem" wx:for-index="imgindex"><image class="close" src="https://www.haojian.cn/wximg/appointment/close2.png" bindtap="inputEvent" data-types="removeImg" data-index="{{index}}" data-imgindex="{{imgindex}}"></image><image class="upimg1" bindtap="inputEvent" data-types="seeBigImg" data-index="{{index}}" data-imgindex="imgindex" src="{{imgItem}}"></image></view><view wx:if="{{item.value.length < 9}}" class="photo_up pos" bindtap="inputEvent" data-types="uploadImgs" data-index="{{index}}" data-num="9"><image class="up_image" src="https://www.haojian.cn/wximg/appointment/add.png"></image></view></view></view><view wx:elif="{{item.type == 'datetime' || item.typeid == 5}}" class="ptb20"><view class="f32 c0 bold">{{item.title}}<text wx:if="{{item.isMust == 1}}" class="color-red f26">*</text></view><view class="flex_between flex_items"><view class="inp form_choose_box mt20 f28 pos flex1"><picker mode="date" start="{{item.value.sd5}}" bindchange="inputEvent" data-types="dateChange" data-datatypes="d" data-index="{{index}}"><view class="picker">{{item.value.d5}}</view><image class="form_rl_icon" src="https://www.haojian.cn/wximg/calendar_icon.png"></image></picker></view><view class="ml30 inp form_choose_box mt20 f28 pos flex1"><picker mode="time" start="{{item.value.st5}}" bindchange="inputEvent" data-types="dateChange" data-datatypes="t" data-index="{{index}}"><view class="picker f32">{{item.value.t5}}</view><image class="down" src="https://www.haojian.cn/wximg/appointment/down.png"></image></picker></view></view></view><view wx:elif="{{item.type == 'date' || item.typeid == 6}}" class="ptb20"><view class="f32 c0 bold">{{item.title}}<text wx:if="{{item.isMust == 1}}" class="color-red f26">*</text></view><view class="inp form_choose_box mt20 f28 pos"><picker mode="date" start="{{item.startDate}}" bindchange="inputEvent" data-types="dateChange" data-datatypes="d" data-index="{{index}}"><view class="picker">{{item.value}}</view><image class="form_rl_icon" src="https://www.haojian.cn/wximg/calendar_icon.png"></image></picker></view></view><view wx:elif="{{item.type == 'time' || item.typeid == 7}}" class="ptb20"><view class="f32 c0 bold">{{item.title}}<text wx:if="{{item.isMust == 1}}" class="color-red f26">*</text></view><view class="inp form_choose_box mt20 f28 pos"><picker mode="time" start="{{item.startTime}}" bindchange="inputEvent" data-types="dateChange" data-datatypes="t" data-index="{{index}}"><view class="picker">{{item.value}}</view><image class="down" src="https://www.haojian.cn/wximg/appointment/down.png"></image></picker></view></view></block>