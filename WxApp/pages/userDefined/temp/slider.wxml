<view class="swiper" style="height:{{item.data.height1}}px"><swiper class="swiper" indicator-dots="true" circular autoplay="true" interval="3000" duration="1200" style="height:{{item.data.height1}}px"><block wx:for-items="{{item.data.list}}" wx:key="" wx:for-item="list"><swiper-item><block wx:if="{{list.url == 'wxAPP'}}"><image lazy-load="true" wx:if="{{list.urlda.code}}" bindtap="openApp" data-url="{{list.urlda.code}}" class="swiper" src="{{list.src}}" mode="scaleToFill" style="height:{{item.data.height1}}px"/><navigator wx:else target="miniProgram" open-type="navigate" app-id="{{list.urlda.appId}}" path="{{list.urlda.page}}" extra-data="" version="release" hover-class="none"><image lazy-load="true" class="swiper" src="{{list.src}}" mode="scaleToFill" style="height:{{item.data.height1}}px"/></navigator></block><image lazy-load="true" wx:else data-imgindex="{{index}}" data-index="{{tempIndex}}" bindtap="openWindow" data-url="{{list.url}}" class="swiper" src="{{list.src}}" mode="scaleToFill" style="height:{{item.data.height1}}px"/></swiper-item></block></swiper></view>