<view class="space"></view><block wx:if="{{kuaidiType === 'self-service'}}"><view wx:if="{{configInfo.ziting}}" class="user_address_box"><image class="address_icon" src="https://www.haojian.cn/wximg/address_icon.png"></image><view class="info_box"><view class="name_phone"><view class="info_name"><text>发货人：</text><text>{{configInfo.express.name}}</text></view><text>{{configInfo.express.mobile}}</text></view><view class="info_address"><text>发货地址：</text><text>{{configInfo.express.citys}}{{configInfo.express.address}}</text></view></view></view></block><block wx:elif="{{kuaidiType === 'normal'}}"><block wx:if="{{addressInfo && addressInfo.name && addressInfo.tel && addressInfo.address}}"><view class="user_address_box" catchtap="changeAddress"><image class="address_icon" src="https://www.haojian.cn/wximg/address_icon.png"></image><view class="info_box"><view class="name_phone"><view class="info_name"><text>收货人：</text><text>{{addressInfo.name}}</text></view><text>{{addressInfo.tel}}</text></view><view class="info_address"><text>收货地址：</text><text>{{addressInfo.address}}</text></view></view><image class="rightIcon" src="https://www.haojian.cn/wximg/more.png"></image></view></block><view wx:else class="confirmAddress bgfff" bindtap="changeAddress"><view class="confirmAddress_add txt_center"><image src="https://www.haojian.cn/wximg/mall/confirm_order_add.png"></image></view><view class="f28 c9 txt_center mt30">您还没有添加收货信息，点击添加</view></view></block>