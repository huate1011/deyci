<view wx:if="{{list.length > 0}}" class="wx-banner mb20"><block wx:if="{{list.length == 1}}"><block wx:if="{{list[0].link == 'wxAPP'}}"><image lazy-load="true" wx:if="{{list[0].urlda.code}}" bindtap="openApp" data-url="{{list[0].urlda.code}}" class="wx-banner" src="{{list[0].image.original_url}}" mode="scaleToFill"/><navigator wx:else target="miniProgram" open-type="navigate" app-id="{{list[0].urlda.appId}}" path="{{list[0].urlda.page}}" extra-data="" version="release" hover-class="none"><image lazy-load="true" class="wx-banner" src="{{list[0].image.original_url}}" mode="scaleToFill"/></navigator></block><image wx:else lazy-load="true" bindtap="openLink" data-types="openWindow" data-url="{{list[0].link}}" class="wx-banner" src="{{list[0].image.original_url}}" mode="scaleToFill"/></block><swiper wx:else class="wx-banner" indicator-dots="true" circular autoplay="true" interval="3000" duration="1000"><block wx:for="{{list}}" wx:key=""><swiper-item><block wx:if="{{item.link == 'wxAPP'}}"><image lazy-load="true" wx:if="{{item.urlda.code}}" bindtap="openApp" data-url="{{item.urlda.code}}" class="wx-banner" src="{{item.image.original_url}}" mode="scaleToFill"/><navigator wx:else target="miniProgram" open-type="navigate" app-id="{{item.urlda.appId}}" path="{{item.urlda.page}}" extra-data="" version="release" hover-class="none"><image lazy-load="true" class="wx-banner" src="{{item.image.original_url}}" mode="scaleToFill"/></navigator></block><image wx:else lazy-load="true" bindtap="openLink" data-types="openWindow" data-url="{{item.link}}" class="wx-banner" src="{{item.image.original_url}}" mode="scaleToFill"/></swiper-item></block></swiper></view>