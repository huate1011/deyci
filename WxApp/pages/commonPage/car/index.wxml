<block wx:if="{{alterRequestStatus}}"><include src="/pages/commonPage/common.wxml"/><banner-html data="{{banner}}"></banner-html><view wx:if="{{gg_content}}"><navigator class="flex_start ptb10 bgfff f24 flex_items overHidden" hover-class="none" url="/pages/common/pages/hongbaoGonglue/hongbaoGonglue?types=car&t=1"><view class="car_index_voice_icon bre9s"><image src="https://www.haojian.cn/wximg/car/voice.png"></image></view><view class="car_index_GongGao cff5"><view id="txt1" style="animation: roll linear {{duration}}s infinite">{{gg_content}}</view></view></navigator></view><view class="box mt20 p30 box_b bgfff flex_around"><view class="bix"><navigator class="flex_between flex_items" hover-class="none" url="/pages/car/pages/Findcar/Findcar?publishType=1"><text>发布找车</text><image class="find_car_image" src="https://www.haojian.cn/wximg/car/car1.png"></image></navigator></view><view class="bix"><navigator class="flex_between flex_items" hover-class="none" url="/pages/car/pages/Findcar/Findcar?publishType=2"><text>发布找人</text><image class="find_per_image" src="https://www.haojian.cn/wximg/car/Personal.png"></image></navigator></view></view><view class="bgfff mt20"><view class="flex_around ptb30 bbe9s flex_items f28"><view wx:for="{{navbar}}" data-idx="{{index}}" class="c_nav pos txt_center {{currentTab==index ? 'cff8' : ''}}" wx:key="unique" bindtap="carEvent" data-types="navbarTap">{{item}}</view><view class="pos txt_center c_nav"><navigator class="flex_center flex_items plr30 box_b" hover-class="none" url="/pages/car/pages/screen/screen"><image class="c_nav_image" src="https://www.haojian.cn/wximg/car/chosee.png"></image><text class="ml10">筛选</text></navigator></view></view></view><block wx:if="{{requestStatus}}"><view wx:if="{{list.length > 0}}" class="t4"><view class="mb20 bgfff pos" wx:for="{{list}}" wx:key=""><navigator url="/pages/car/pages/detail/detail?id={{item.id}}" hover-class="none"><image wx:if="{{item.isTop == 1}}" class="car_zhiding" src="https://www.haojian.cn/wximg/car/top.png"></image><view class="flex_start flex_items p30 box_b"><view class="vacancy cf f26 txt_center" style="background:#{{item.publishType==1 ? peise.mcolor : peise.acolor}}">{{item.publishType==1 ? "人找车" : "车找人"}}</view><view wx:if="{{item.publishType==1}}" class="ml10 f30">{{item.peopleNum}}人乘车</view><view wx:else class="ml10 f28 c3">{{item.peopleNum}}个空位</view></view><view class="flex_around f30 txt_center p30 box_b bbe9s"><view class="bold" style="width:262rpx">{{item.startPoint}}</view><view class="Arrow flex_shrink"><view><image class="Trip_image" src="https://www.haojian.cn/wximg/car/arrow.png"></image></view><view class="txt_center f22 mt10" style="color:#{{peise.mcolor}}">{{item.Ymd}} {{item.His}}</view></view><view class="bold" style="width:226rpx">{{item.endPoint}}</view></view></navigator><view class="f30 plr30 pb30 box_b"><view wx:if="{{item.viaPoint}}" class="mt20" style="color:#454545">途径地：{{item.viaPoint}}</view><view wx:if="{{item.carModel}}" class="mt20" style="color:#454545">车型：{{item.carModel}}</view><view wx:if="{{item.remark}}" class="mt20 f30 flex_start" style="color:#454545"><view class="flex_shrink">备注：</view><view>{{item.remark}}</view></view><view class="bei mt20 flex_between flex_items"><view class="flex_start flex_items"><image class="bei_image" src="https://www.haojian.cn/wximg/car/sex{{item.gender}}.png"></image><view class="ml15 f26 c9">{{item.uname}}</view><view class="ml30 f26 c9">{{item.mobile}}</view></view><button class="contactbutton cf flex_around flex_items plr20 box_b" bindtap="carEvent" data-types="calling" data-index="{{index}}" style="background:#{{peise.mcolor}}"><image class="contactbutton_phone" src="https://www.haojian.cn/wximg/car/phone1.png"></image><text class="ml10 f26">联系TA</text></button></view></view></view></view><view wx:if="{{list.length == 0}}" class="noDetail">没有数据</view><view wx:if="{{list.length != 0 && !hasNext}}" class="noNore">没有更多数据了</view></block></block>