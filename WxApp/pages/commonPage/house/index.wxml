<block wx:if="{{alterRequestStatus}}"><include src="/pages/commonPage/common.wxml"/><banner-html data="{{banner}}"></banner-html><view wx:if="{{switch_menus.length > 0}}" class="ptb20 plr30 box_b"><view class="flex_between flex_items bgfff h_search1"><picker bindchange="houseEvent" data-types="housePickerChange" value="{{index}}" range-key="menu_name" range="{{switch_menus}}"><view class="h_search11 flex_around flex_items"><view class="f24 c6">{{searchdedefaultvalue}}</view><image class="h_search11_down" src="https://www.haojian.cn/wximg/house/down.png"></image></view></picker><view class="h_search12 flex1 f24 c9"><input placeholder="搜索关键词：如日租" type="text" bindinput="houseEvent" data-types="housesearchInput"/></view><view class="h_search13" style="background:#{{peise.mcolor}}" bindtap="houseEvent" data-types="housesearch"><image src="https://www.haojian.cn/wximg/house/search_input.png"></image></view></view></view><view class="flex_warp pt30 bgfff"><view class="h_nav txt_center mb30" wx:for="{{visiblemenus}}" wx:key="" bindtap="houseEvent" data-types="houseentermenu" data-index="{{item.menu_id}}"><image lazy-load="true" mode="scaleToFill" class="h_nav_img" src="{{item.menu_img}}"></image><view class="f30 c6">{{item.menu_name}}</view></view><view wx:if="{{isagent_status == 1}}" class="h_nav txt_center mb30" bindtap="houseEvent" data-types="houseentermenu" data-index="9"><image mode="scaleToFill" class="h_nav_img" src="https://www.haojian.cn/wximg/house/jjr.png"></image><view class="f30 c6">经纪人发布</view></view></view><view class="mt20 ptb30 bgfff" wx:if="{{switch_menus.length >1 }}"><view class="h_type flex_between txt_center f30 c3" style="border-color:#{{peise.mcolor}}"><view bindtap="houseEvent" wx:for="{{switch_menus}}" wx:key="" data-types="nav" data-index="{{item.menu_id}}" class='flex1 {{switch_menus_status == item.menu_id? "cf" : ""}}' style='background:#{{switch_menus_status == item.menu_id? peise.mcolor : ""}}'>{{switch_menus[index].menu_name}}</view></view></view><block wx:if="{{requestStatus}}"><block wx:if="{{list.length > 0}}"><view class="bgfff" wx:for="{{list}}" wx:key=""><navigator url="/pages/house/pages/detail/detail?id={{item.id}}" hover-class="none"><view class="bte9s p30 b0x_b flex_start"><view class="pos h_list_img flex_shrink pos"><image lazy-load="true" mode="scaleToFill" class="h_list_img" src="{{item.img.thumb_small_url}}"></image><view wx:if="{{item.isTop==1}}"><view class="h_list_zd cf txt_center f20" style="background:#{{peise.mcolor}}">置顶</view></view></view><view class="flex1 pl20 box_b"><view class="f28 c3 clamp1">{{item.title}}</view><view class="flex_between flex_items mt10" wx:if="{{item.demand_type==1 || item.demand_type==2}}"><view wx:if="{{item.model}}" class="f24 c6">{{item.model}} {{item.acreage}}㎡</view><view wx:elif="{{item.acreage}}" class="f24 c6">{{item.acreage}}㎡</view><view class="f32 cff5">{{item.price}}</view></view><view class="flex_between flex_items mt10" wx:if="{{item.demand_type==8}}"><view wx:if="{{item.housetype}}" class="f24 c6">{{item.housetype}} {{item.acreage}}㎡</view><view wx:elif="{{item.acreage}}" class="f24 c6">{{item.acreage}}㎡</view><view class="f32 cff5">{{item.price}}/㎡</view></view><view class="flex_between flex_items mt10"><view class="flex_start flex_items pr20 box_b"><image class="h_list_loc flex_shrink" src="https://www.haojian.cn/wximg/house/Location.png"></image><view class="f24 c9 clamp1 ml20">{{item.address}}</view></view><view class="f20 c9 flex_shrink">{{item.updatetime}}</view></view><view class="flex_start f22 mt10"><view class="h_list_desc mr10" wx:for="{{item.label}}" data-index="{{index}}" wx:key="">{{item}}</view></view></view></view></navigator></view></block><view wx:if="{{list.length == 0}}" class="noDetail">没有数据</view><view wx:if="{{list.length != 0 && !hasNext}}" class="noNore">没有更多数据了</view><block wx:if="{{list.length == 0}}"><copyright-html data="{{config}}"></copyright-html></block></block></block>