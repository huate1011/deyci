<view wx:if="{{list.length > 0}}"><view class="detail_ad"><view class="detail_ad1"></view><view class="detail_ad2">广告</view></view><view wx:for="{{list}}" wx:key="" class="detail_ad_Img"><block wx:if="{{item.link == 'wxAPP'}}"><image lazy-load="true" bindload="ImgLoad" data-index="{{index}}" wx:if="{{item.urlda.code}}" bindtap="openApp" data-url="{{item.urlda.code}}" class="wx-banner" src="{{item.image.original_url}}" mode="scaleToFill" style="height:{{item.height}}rpx"/><navigator wx:else target="miniProgram" open-type="navigate" app-id="{{item.urlda.appId}}" path="{{item.urlda.page}}" extra-data="" version="release" hover-class="none"><image lazy-load="true" bindload="ImgLoad" data-index="{{index}}" class="wx-banner" src="{{item.image.original_url}}" mode="scaleToFill" style="height:{{item.height}}rpx"/></navigator></block><image wx:else lazy-load="true" bindload="ImgLoad" bindtap="openLink" data-types="openWindow" data-index="{{index}}" data-url="{{item.link}}" src="{{item.image.original_url}}" mode="scaleToFill" style="height:{{item.height}}rpx"/></view></view>