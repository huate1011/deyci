<view class="my"><view class="my1 pos txt_center pos" style="background:#{{peise.mcolor}}"><button wx:if='{{myInfo.avatar_url.thumb_small_url == "" && myInfo.displayname == ""}}' open-type="getUserInfo" bindgetuserinfo="userInfoHandler" class="getUserInfo f30 c3 bgfff">点击授权</button><view wx:else><image class="my1_1 radius" mode="scaleToFill" src="{{myInfo.avatar_url.thumb_small_url}}"></image><view class="my1_2 f34 mt30 cf">{{myInfo.displayname}}</view></view><view class="my1_4 bgfff txt_left"><navigator url="/pages/common/pages/editPersonInfo/editPersonInfo" hover-class="none"><view class="pos f22 flex_items"><view class="pl20" style="color:#{{peise.mcolor}}">编辑资料</view><image class="my1_4_1_2" src="{{peise.editIcon}}"></image></view></navigator></view></view><block wx:if="{{menuStatus}}"><view class="flex_warp bgfff pb30"><view wx:if="{{menuList.myIsPoint == 1}}" class="my_nav mt30"><navigator url="/pages/market/pages/integral/myIntegral/myIntegral" hover-class="none"><image class="my_nav1" src="https://www.haojian.cn/wximg/my_point.png"></image><view class="my_nav2 f24 cb txt_center">{{mypoint}}</view><view class="f26 c6 txt_center">{{menuList.myPointName}}</view></navigator></view><view wx:if="{{menuList.myIsMember == 1}}" class="my_nav mt30"><navigator url="/pages/market/pages/card/myCard/myCard" hover-class="none"><image class="my_nav1" src="https://www.haojian.cn/wximg/my_hy.png"></image><view class="my_nav2 f24 cb txt_center">{{membName}}</view><view class="f26 c6 txt_center">{{menuList.myMemberName}}</view></navigator></view><view wx:if="{{menuList.myIsCashcard == 1}}" class="my_nav mt30"><navigator url="/pages/market/pages/recharge/index/index" hover-class="none"><image class="my_nav1" src="https://www.haojian.cn/wximg/my_wallet.png"></image><view class="my_nav2 f24 cb txt_center">¥{{cash_balance}}</view><view class="f26 c6 txt_center">{{menuList.myCashcardName}}</view></navigator></view><view wx:if="{{menuList.myIsMoney == 1}}" class="my_nav mt30"><navigator url="/pages/common/pages/myWallet/myWallet" hover-class="none"><image class="my_nav1" src="https://www.haojian.cn/wximg/my_hb.png"></image><view class="my_nav2 f24 cb txt_center">¥{{hongbao.balance}}</view><view class="f26 c6 txt_center">{{menuList.myMoneyName}}</view></navigator></view><view wx:if="{{menuList.underlinePay == 1}}" class="my_nav mt30"><navigator url="/pages/common/pages/underlineCheck/underlineCheck" hover-class="none"><image class="my_nav1" src="https://www.haojian.cn/wximg/xxPay.png"></image><view class="my_nav2 f24 cb txt_center"></view><view class="f26 c6 txt_center">线下买单</view></navigator></view></view><block wx:for="{{menuList.list}}" wx:key=""><view class="bgfff mt20 pl30 box_b"><view wx:if="{{item.classList.length > 0}}" bindtap="myEvent" data-types="nav" data-index="{{index}}" class="pos ptb30 flex_start flex_items {{item.show ? 'bbe9s' : ''}}"><image class="leftIcon flex_shrink" src="{{item.icon}}"></image><view class="f32 c3 ml20">{{item.name}}</view><image wx:if="{{item.show}}" class="downIcon" src="https://www.haojian.cn/wximg/my/downIcon.png"></image><image wx:if="{{!item.show}}" class="rightIcon" src="https://www.haojian.cn/wximg/right_icon.png"></image></view><block wx:if="{{item.show && item.classList.length > 0}}"><block wx:for="{{item.classList}}" wx:key="" wx:for-item="itemList" wx:for-index="itemIndex"><view class="pos {{item.classList.length -1 > itemIndex ? 'bbe9s' : ''}}"><navigator url="{{itemList.url}}" hover-class="none" class="ptb30 flex_start flex_items"><image class="pointIcon" src="https://www.haojian.cn/wximg//my/point.png"></image><view class="f32 c3 ml30">{{itemList.name}}</view><view wx:if="{{itemList.desc}}" class="my4_3 f26 c9">{{itemList.desc}}</view><image class="rightIcon" src="https://www.haojian.cn/wximg/right_icon.png"></image></navigator></view></block></block><view wx:if="{{item.classList.length == 0}}" class="pos flex_items"><navigator url="{{item.url}}" hover-class="none" class="ptb30 flex_start flex_items"><image class="leftIcon" src="{{item.icon}}"></image><view class="f32 c3 ml20">{{item.name}}</view><image class="rightIcon" src="https://www.haojian.cn/wximg/right_icon.png"></image><block wx:if="{{item.type == 'message'}}"><view wx:if="{{myInfo.unread_message_count > 0 }}" class="my2_4 radius f20 cf txt_center">{{myInfo.unread_message_count}}</view></block></navigator></view></view></block></block></view><block wx:if="{{menuStatus}}"><copyright-html data="{{config}}"></copyright-html></block>