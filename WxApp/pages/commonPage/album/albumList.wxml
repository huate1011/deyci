<include src="/pages/commonPage/common.wxml"/><block wx:if="{{requestStatus}}"><view class="album"><view wx:if="{{view_type === 'normal'}}" class="albumList"><block wx:for="{{list}}" wx:key=""><view class="albumItem" bindtap="albumEvent" data-types="viewBigImg" data-id="{{item.id}}" data-total="{{item.photo_count}}"><image lazy-load="true" mode="scaleToFill" src="{{item.cover.thumb_medium_url}}"></image><view class="title_0">{{item.name}}</view><view class="f24 c9 pl15">共{{item.photo_count}}张</view></view></block></view><view wx:else class="albumContainer"><scroll-view scroll-x="{{true}}" class="albumList_1" bindscrolltolower="albumEvent" data-types="scrolltolower"><block wx:for="{{list}}" wx:key=""><view class="albumItem_1" bindtap="albumEvent" data-types="viewBigImg" data-id="{{item.id}}" data-total="{{item.photo_count}}"><image lazy-load="true" src="{{item.cover.original_url}}" mode="scaleToFill"></image><view class="text_box"><view class="title_1">{{item.name}}</view><view class="albumItem_1_info"><text class="f28 c9" style="text-align: left">共{{item.photo_count}}张</text><text class="f22 cc">{{item.datetime}}</text></view></view><view class="showShadow"></view></view></block></scroll-view></view></view><view wx:if="{{list.length == 0}}" class="noRow">暂未添加相册！</view></block>