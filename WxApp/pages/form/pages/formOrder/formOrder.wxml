<block wx:if="{{requestStatus}}"><block wx:if="{{list.length > 0}}"><view wx:for="{{list}}" wx:key="" class="bgfff mt20"><navigator class="plr30" url="/pages/form/pages/orderDetail/orderDetail?id={{item.id}}&index={{index}}" hover-class="none"><view class="status_box flex_between flex_items bbe9s"><view class="f22 c6">提交时间：{{item.addtime}}</view><view wx:if="{{item.shopOrderId > 0}}" class="f24 cff5">{{item.orderStatusId === 'wait_pay' ? '待支付' : '已支付'}}</view><view wx:else class="f24 cff5">免费</view></view><view class="flex_start flex_items ptb30"><image class="flex_shrink order_img" src="{{item.img.thumb_medium_url}}"></image><view><view class="f28 c6">{{item.title}}</view><view class="mt20 f22 c9">有效期至：{{item.endTime}}</view></view></view></navigator><view class='footer_box plr30 {{item.shopOrderId > 0 ? "flex_between" : "flex_end"}} flex_items bte9s'><view class="f26 c3" wx:if="{{item.shopOrderId > 0}}">合计：<text class="cff5">￥{{item.orderPrice}}</text></view><view class="btn_box flex_start txt_center"><block wx:if="{{item.shopOrderId > 0 && item.orderStatusId === 'wait_pay'}}"><form wx:if="{{iphoneShow}}" report-submit="true" bindsubmit="changeStatusEven" data-types="pay" data-index="{{index}}"><button form-type="submit" class="btn mr20 cf f26" style="background:#{{peise.mcolor}}">立即支付</button></form><view class="btn be9s c6 f26" catchtap="changeStatusEven" data-types="del" data-index="{{index}}">删除订单</view></block><view wx:else class="btn be9s c6 f26" catchtap="changeStatusEven" data-types="del" data-index="{{index}}">删除订单</view></view></view></view></block><view wx:else class="noComment">暂无数据</view><view wx:if="{{list.length != 0 && !hasNext}}" class="noNore">没有更多数据了</view><block wx:if="{{list.length === 0}}"><copyright-html data="{{config}}"></copyright-html></block></block><block wx:if="{{payAndBingPhoneStatus}}"><include src="/pages/commonPage/payTemp.wxml"/><include src="/pages/commonPage/common/setPassword.wxml"/><include src="/pages/commonPage/common/bindPhone.wxml"/></block>